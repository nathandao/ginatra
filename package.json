{
  "name": "ginatra",
  "version": "1.0.0",
  "description": "Ginatra is a team dashboard that displays git repository statistics in real time using Sinatra and ReactJs",
  "main": "assets/js/app.js",
  "scripts": {
    "build": "browserify -t reactify assets/js/app.jsx -o assets/js/bundle.js",
    "watch": "watchify -t reactify assets/js/app.jsx -o assets/js/bundle.js",
    "lint": " ./node_modules/.bin/eslint assets/js/*.jsx assets/js/*/**.jsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nathandao/ginatra.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nathandao/ginatra/issues"
  },
  "homepage": "https://github.com/nathandao/ginatra",
  "devDependencies": {
    "browserify": "^11.0.1",
    "eslint": "^1.7.3",
    "eslint-plugin-react": "^3.6.3",
    "reactify": "^1.1.1",
    "watchify": "^3.2.1"
  },
  "dependencies": {
    "chart.js": "^1.0.2",
    "jquery": "^2.1.4",
    "react": "^0.13.3",
    "react-chartjs": "nathandao/react-chartjs"
  }
}
